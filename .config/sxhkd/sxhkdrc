# My Hotkeys

Print
	maim --select | tee ~/pix/ss/$(date +%s).jpg | xclip -sel c -t image/jpg

super + Print 
	maim | tee ~/pix/ss/$(date +%s).jpg | xclip -sel c -t image/jpg

XF86Audio{RaiseVolume,LowerVolume,Mute}
	pamixer -{i,d,t} 2 ;

XF86MonBrightness{Up,Down}
	brightnessctl s {+2%,2%-}

super + Escape
	system_action
super + x
	rofi -show drun
super + shift + x
	rofi -show run
